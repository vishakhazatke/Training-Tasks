package Day3;

class Demo{
	
	void Student(int id, String name) {
		
	}
	
	void Student(int id, String name, int age) {
		
	}
}
public class MethodOverloadingDemo {

	public static void main(String[] args) {

		Demo d1 = new Demo();
		d1.Student(1, "Vishakha");
		
		Demo d2 = new Demo();
		d2.Student(2, "Vishu", 22);
	}
}
