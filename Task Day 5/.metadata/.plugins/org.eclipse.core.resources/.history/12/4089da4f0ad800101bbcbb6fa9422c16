package Day5;

class AnimalDemo {
	
    void sound() {
        System.out.println("Animal makes a sound");
    }
}

class DogDemo extends AnimalDemo {
	
    @Override
    void sound() {
        System.out.println("Dog barks");
    }
}

public class UpcastingDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
