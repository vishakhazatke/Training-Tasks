package Day5;

class Animal {
	
    void sound() {
        System.out.println("Animal makes a sound");
    }
}

class Dog extends Animal {
	
    @Override
    void sound() {
        System.out.println("Dog barks");
    }
}

public class UpcastingDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
