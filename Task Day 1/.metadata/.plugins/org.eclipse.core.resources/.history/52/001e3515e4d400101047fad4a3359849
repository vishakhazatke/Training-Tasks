package Day1;

class Student{
	
	int id;
	String name;
	double marks;
	
	public Student(int id, String name, double marks) {
		super();
		this.id = id;
		this.name = name;
		this.marks = marks;
	}
	
	void StudentDetails() {
		System.out.println("The ID of student is :" + id + "Name of Student is "+ name + "and Student marks is " + marks);
	}
	
}
public class StudentDemo {

	public static void main(String[] args) {
		
		Student s1 = new Student(1, "Vishakha", 75.5);
		s1.StudentDetails();
	}
}
