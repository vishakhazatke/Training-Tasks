package Day1;

class Student{
	
	int id;
	String name;
	double marks;
	
	public Student(int id, String name, double marks) {
		super();
		this.id = id;
		this.name = name;
		this.marks = marks;
	}
	
	
}
public class StudentDemo {

	public static void main(String[] args) {
		
		Student s1 = new Student(1, "Vishakha", 75.5);
	}
}
